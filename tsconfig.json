{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "target": "ES6",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "jsx": "react-native",
    "lib": ["DOM", "ESNext"],
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
    }
  },
  "include": [
    "**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "eslint.config.mjss", "eslint.config.mjsjs",
  ],
  "exclude": ["node_modules"]
}
